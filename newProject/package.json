  "simple-git-hooks": {
    "post-merge": "git submodule update --init --recursive",
    "post-rewrite": "git submodule update --init --recursive",
    "post-checkout": "git submodule update --init --recursive"
  },
  "overrides": {},
  "nano-staged": {
    "{src,app,modules}/**/*.{js,ts,jsx,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "expo": {
    "autolinking": {
      "android": {
        "exclude": [
          "expo-application"
        ]
      },
      "ios": {
        "flags": {
          "inhibit_warnings": true
        }
      }
    }
